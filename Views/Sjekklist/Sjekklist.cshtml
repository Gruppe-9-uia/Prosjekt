@model SjekklisteModel;
@{
    ViewData["Title"] = "Sjekkliste";
}


<div class="p-3">
    <h2>Nøsted&, avd. Jåbekk</h2>
    <h3>Kvalitetsskring</h3>
    <h3>Registrering</h3>
</div>


<div class="container p-3">
    <div class="row">
        <div class="col-sm text-center">
            <h4>Prosedyre:</h4>
            <p>Servicesjekkliste vinsjer</p>
        </div>
        <div class="col-sm text-center">
            <h4>Dokumenter number:</h4>.step active
            <!-- Det skal bli hente fra database når man lager nytt sjekkliste -->
            <p>IG-IN-104-01-01</p>
        </div>
        <div class="col-sm text-center">
            <h4>Dato</h4>
            <!-- Dette skal også bli hente fra databasen -->
            <p>24.20.2023</p>
        </div>
        <div class="col-sm text-center">
            <h4>Utarbeidet av:</h4>
            <!-- Dette skal også bli hente fra databasen -->
            <p>JBS</p>
        </div>
    </div>
</div>

<!-- hentet og modifsert fra https://codepen.io/Johann-S/pen/yQKmMG?editors=1010 -->
<div id="stepperSjekkliste" class="bs-stepper">
    <div class="bs-stepper-header flex-wrap" role="tablist">
        <div class="step" data-target="#Generell">
            <button type="button" class="btn btn-link step-trigger" role="tab" id="stepperSjekkliste1" aria-controls="Generell">
                <span class="bs-stepper-circle">1</span>
                <span class="bs-stepper-label">Generell</span>
            </button>
        </div>
        <div class="line"></div>
        <div class="step" data-target="#Mekanisk">
            <button type="button" class="btn btn-link step-trigger" role="tab" id="stepperSjekkliste2" aria-controls="Mekanisk">
                <span class="bs-stepper-circle">2</span>
                <span class="bs-stepper-label">Mekanisk</span>
            </button>
        </div>
        <div class="line"></div>
        <div class="step" data-target="#Hydraulisk">
            <button type="button" class="btn btn-link step-trigger" role="tab" id="stepperSjekkliste3" aria-controls="Hydraulisk">
                <span class="bs-stepper-circle">3</span>
                <span class="bs-stepper-label">Hydraulisk</span>
            </button>
        </div>
        <div class="line"></div>
        <div class="step" data-target="#Elektro">
            <button type="button" class="btn btn-link step-trigger" role="tab" id="stepperSjekkliste4" aria-controls="Elektro">
                <span class="bs-stepper-circle">4</span>
                <span class="bs-stepper-label">Elektro</span>
            </button>
        </div>
        <div class="line"></div>
        <div class="step" data-target="#Signering">
            <button type="button" class="btn btn-link step-trigger" role="tab" id="stepperSjekkliste5" aria-controls="Signering">
                <span class="bs-stepper-circle">5</span>
                <span class="bs-stepper-label">Signering</span>
            </button>
        </div>
    </div>
    <div class="bs-stepper-content">
            <!-- Generell informasjon som er ikke avhengig av avdeling-->
            <div id="Generell" role="tabpanel" class="bs-stepper-pane active dstepper-block" aria-labelledby="stepperSjekkliste1">
                <div class="form-group">
                    <div class="p-3">
                    @using (Html.BeginForm("GenerrellForm", "Sjekklist"))
                    {
                            @Html.AntiForgeryToken()
                            <div class="form-group">
                                <label for="serieNr">Serie Number:</label>
                                <input type="text" class="form-control" id="serieNr" asp-for="Serial_number_str">
                            </div>

                            <div class="form-group">
                                <label for="type">Type:</label>
                                <input type="text" class="form-control" id="type" asp-for="Type_str">
                            </div>

                            <h5>Trykk settninger</h5>
                            <div class="form-group">
                                <label for="xx_bar">xx-Bar:</label>
                                <input type="text" class="form-control" id="xx_bar" asp-for="xx_Bar_str">
                            </div>

                            <h5>Trykk settninger</h5>
                            <div class="form-group">
                                <label for="test-vnsj">Test vinsj og kjør alle funskjoner:</label>
                                <input type="text" class="form-control" id="test-vnsj" asp-for="Test_winch">
                            </div>
                            <div class="form-group">
                                <label for="rekkratt">I rekkratt KN:</label>
                                <input type="text" class="form-control" id="rekkratt" asp-for="Traction_force_Kn">
                            </div>
                            <div class="form-group">
                                <label for="bremseKraft">Bremse kraft KN:</label>
                                <input type="text" class="form-control" id="bremseKraft" asp-for="Brake_force">
                            </div>

                            <div class="form-group">
                                <label for="kommentar"><h5>Kommentar:</h5></label>
                                <textarea class="form-control" rows="5" id="kommentar" asp-for="comment_str"></textarea>
                            </div>
                    <div class="row p-3 w-50 align-items-center">
                            <button type="submit" class="col-sm btn btn-primary m-2">Oppdater</button>
                            <button type="button" class="col-sm btn btn-primary m-2" onclick="stepper1.next()">Next</button>
                    </div>
                       }    
                    </div>
                </div>
            </div>

